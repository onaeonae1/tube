extends layouts/main.pug
include mixins/videoBlock.pug

block content
    .search_header
        h3 searching for:  #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            h4 #{item.videoFile}
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.videoFile,
                id:item.id
            })